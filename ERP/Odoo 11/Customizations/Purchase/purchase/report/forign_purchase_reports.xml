<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <report 
            string="Foreign Purchase Order" 
            id="action_report_purchase_order" 
            model="forign.purchase.order" 
            report_type="qweb-pdf"
            name="purchase.forign_report_purchaseorder" 
            file="purchase.forign_report_purchaseorder"
            print_report_name="
                (object.state in ('draft', 'sent') and 'Request for Quotation - %s' % (object.name) or
                'Purchase Order - %s' % (object.name))"
        />

        <report 
            string="Foreign Purchase Request for Quotation" 
            id="forign_report_purchase_quotation" 
            model="forign.purchase.order" 
            report_type="qweb-pdf"
            name="purchase.forign_report_purchasequotation" 
            file="purchase.forign_report_purchasequotation"
            print_report_name="'Request for Quotation - %s' % (object.name)"
        />
    </data>
</odoo>
