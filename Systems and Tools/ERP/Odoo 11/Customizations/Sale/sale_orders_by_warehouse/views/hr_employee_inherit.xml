<odoo>
  <record id="view_hr_employee_form_inherit_dispatcher" model="ir.ui.view">
    <field name="name">hr.employee.form.inherit.dispatcher</field>
    <field name="model">hr.employee</field>
    <field name="inherit_id" ref="hr.view_employee_form"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='manager']" position="after">
        <field name="is_parcel_dispatcher"/>
      </xpath>
    </field>
  </record>
</odoo>
